{
  "name": "mjml-templating-api",
  "version": "1.0.0",
  "description": "MJML Templating API",
  "repository": "https://github.com/eugene-bert/mjml-templating-api",
  "main": "dist/index.js",
  "scripts": {
    "format": "./node_modules/prettier/bin-prettier.js --write \"src/**/*.js\"",
    "style": "./node_modules/eslint/bin/eslint.js --fix \"src/**/*.js\"",
    "build": "./node_modules/@babel/cli/bin/babel.js src --out-dir dist",
    "start": "nodemon --exec ./node_modules/@babel/node/bin/babel-node.js src/index.js"
  },
  "dependencies": {
    "body-parser": "1.20.2",
    "express": "4.18.2",
    "express-prom-bundle": "6.6.0",
    "mjml": "4.14.1",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prom-client": "14.2.0",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@babel/cli": "7.21.5",
    "@babel/core": "7.22.1",
    "@babel/eslint-parser": "7.21.8",
    "@babel/node": "7.22.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "7.22.2",
    "@babel/register": "7.21.0",
    "@babel/runtime": "7.22.0",
    "babel-eslint": "^10.1.0",
    "eslint": "8.41.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "fancy-log": "^1.3.3",
    "prettier": "2.8.8"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
